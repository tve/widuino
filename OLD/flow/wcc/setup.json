{
    "init": {
        "gadgets": [
            {
                "name": "dummy",
                "type": "Pipe"
            },
            {
                "name": "driverFill",
                "type": "driverFill"
            },
            {
                "name": "tableFill",
                "type": "tableFill"
            },
            {
                "name": "sub2",
                "type": "DataSub"
            },
            {
                "name": "aggr",
                "type": "Aggregator"
            },
            {
                "name": "db",
                "type": "LevelDB"
            },
            {
                "name": "udpgw",
                "type": "udpgw"
            }
        ],
        "wires": [
            {
                "from": "sub2.Out",
                "to": "aggr.In"
            },
            {
                "from": "aggr.Out",
                "to": "db.In"
            }
        ],
        "feeds": [
            {
                "data": "sensor/",
                "to": "sub2.In"
            }
        ],
        "labels": [
            {
                "external": "In",
                "internal": "dummy.In"
            },
            {
                "external": "Out",
                "internal": "dummy.Out"
            }
        ]
    },
    "WebSocket-jeebus": {
        "gadgets": [
            {
                "name": "rpc",
                "type": "RpcHandler"
            }
        ],
        "labels": [
            {
                "external": "In",
                "internal": "rpc.In"
            },
            {
                "external": "Out",
                "internal": "rpc.Out"
            }
        ]
    },
    "udpgw": {
        "gadgets": [
            {
                "name": "udp",
                "type": "UDP-Gateway"
            },
            {
                "name": "f1",
                "type": "FanOut"
            },
            {
                "name": "lg",
                "type": "Logger"
            },
            {
                "name": "db",
                "type": "rf12toDatabase"
            },
            {
                "name": "jb",
                "type": "Booter"
            }
        ],
        "wires": [
            {
                "from": "udp.Recv",
                "to": "f1.In"
            },
            {
                "from": "f1.Out:lg",
                "to": "lg.In"
            },
            {
                "from": "f1.Out:db",
                "to": "db.In"
            },
            {
                "from": "udp.Oob",
                "to": "jb.In"
            },
            {
                "from": "jb.Out",
                "to": "udp.Xmit"
            }
        ],
        "feeds": [
            {
                "data": 9999,
                "to": "udp.Port"
            },
            {
                "data": "./logger",
                "to": "lg.Dir"
            }
        ]
    },
    "Booter": {
        "gadgets": [
            {
                "name": "jb",
                "type": "JeeBoot"
            },
            {
                "name": "wf",
                "type": "WatchFile"
            },
            {
                "name": "cf",
                "type": "ReadFileJSON"
            },
            {
                "name": "rd",
                "type": "ReadFileText"
            },
            {
                "name": "hx",
                "type": "IntelHexToBin"
            },
            {
                "name": "bf",
                "type": "BinaryFill"
            },
            {
                "name": "cs",
                "type": "CalcCrc16"
            },
            {
                "name": "bd",
                "type": "BootData"
            }
        ],
        "wires": [
            {
                "from": "cf.Out",
                "to": "jb.Cfg",
                "capacity": 10
            },
            {
                "from": "jb.Files",
                "to": "wf.In",
                "capacity": 10
            },
            {
                "from": "wf.Out",
                "to": "rd.In",
                "capacity": 10
            },
            {
                "from": "rd.Out",
                "to": "hx.In",
                "capacity": 100
            },
            {
                "from": "hx.Out",
                "to": "bf.In",
                "capacity": 100
            },
            {
                "from": "bf.Out",
                "to": "cs.In",
                "capacity": 100
            },
            {
                "from": "cs.Out",
                "to": "bd.In",
                "capacity": 100
            }
        ],
        "labels": [
            {
                "external": "In",
                "internal": "jb.In"
            },
            {
                "external": "Out",
                "internal": "jb.Out"
            }
        ],
        "feeds": [
            {
                "data": "jeeboot.json",
                "to": "cf.In"
            },
            {
                "data": 64,
                "to": "bf.Len"
            }
        ]
    },
    "nodesWCC": {
        "gadgets": [
            {
                "name": "nm",
                "type": "NodeMapM"
            },
            {
                "name": "di",
                "type": "PacketMapDispatcher"
            }
        ],
        "feeds": [
            {
                "data": "sketch",
                "to": "di.Field"
            },
            {
                "data": "Sketch-",
                "to": "di.Prefix"
            },
            {
                "data": "RFg212i14,tempNode,desk",
                "to": "nm.Info"
            }
        ],
        "wires": [
            {
                "from": "nm.Out",
                "to": "di.In"
            }
        ],
        "labels": [
            {
                "external": "In",
                "internal": "nm.In"
            },
            {
                "external": "Out",
                "internal": "di.Out"
            }
        ]
    },
    "modules": {
        "gadgets": [
            {
                "name": "mi",
                "type": "ModuleIdent"
            },
            {
                "name": "mm",
                "type": "ModuleMap"
            },
            {
                "name": "di",
                "type": "PacketMapDispatcher"
            }
        ],
        "wires": [
            {
                "from": "mi.Out",
                "to": "mm.In"
            },
            {
                "from": "mm.Out",
                "to": "di.In"
            }
        ],
        "feeds": [
            {
                "data": "1,Net",
                "to": "mm.Info"
            },
            {
                "data": "2,Log",
                "to": "mm.Info"
            },
            {
                "data": "4,OwTemp",
                "to": "mm.Info"
            },
            {
                "data": "Module-",
                "to": "di.Prefix"
            },
            {
                "data": "module_name",
                "to": "di.Field"
            }
        ],
        "labels": [
            {
                "external": "In",
                "internal": "mi.In"
            },
            {
                "external": "Out",
                "internal": "di.Out"
            }
        ]
    },
    "rf12toDatabase": {
        "gadgets": [
